*{margin:0; padding:0; font-family: 'Noto Sans KR', sans-serif;}
ul,ol{list-style:none;}
a,a:link{text-decoration:none; color:#000;}
html, body { box-sizing: border-box; padding: 0; margin: 0;}
.clearfix:after { content: ''; display: block; clear: both; float: none; }
img{vertical-align:top;}
html{margin:0 auto;}
.com{float:right;font-family: 'Nanum Myeongjo', serif;font-weight:800;font-size:0.9em;opacity:0.6;padding-top:3px;}
.line{width:90%;margin:0 auto;border-bottom:1px solid #000;}
.underline {background-repeat: no-repeat; background-size: 0% 100%;	transition: background-size 0.4s; color: #000; cursor: pointer;}
.underline.yellow {background-image: linear-gradient(to bottom,transparent 80%, #F8CD07 100%, white);}
.underline.red {background-image: linear-gradient(to bottom,transparent 80%, #ef3837 100%, white);}
.underline.blue{background-image: linear-gradient(to top,transparent 30%,rgba(227,165,7,1) 100%);background-size: 100% 0%;}
.underline:hover {background-size: 100% 100%;}

