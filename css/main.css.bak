	/*★★★★★★★☆☆☆☆☆☆☆PARALLAX☆☆☆☆☆☆☆★★★★★★★*/
		.parallax{
			position: relative;
			margin:0 auto;
			width: 100%;
			box-sizing: border-box;
			transform-style: inherit;
			overflow:hidden;
			height:250px;
			perspective: 1px;
			transform-style: preserve-3d;
			cursor:pointer;
			transition:.4s;
		}
		.parallax:before{
			background:no-repeat fixed center / cover;
			transform:translateZ(-1.7px)scale(3);
			content: "";
			position: absolute;
			top: 0;
			bottom: 0;
			left: 0;
			right: 0;
		}
		.parallax.boxChange{
			width:90%;
			margin:10px auto 0;
			height:120px;
			/*-webkit-box-shadow: 0px 0px 9px 0px rgba(0,0,0,0.85);
			-moz-box-shadow: 0px 0px 9px 0px rgba(0,0,0,0.85);
			box-shadow: 0px 0px 9px 0px rgba(0,0,0,0.85);*/
		}
		.gara:before{
			background-image:url(../images/garage3.jpg)
		}
		.hermi:before{
			background-image:url(../images/hermitage.jpg);
		}
		.cosmo:before{
			background-image:url(../images/cosmonautics4.jpg);
		}
		.p-inner{
